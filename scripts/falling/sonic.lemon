function u64 stringformat(u64 format, u64 arg1)
{
    if (format == "character_sonic_0x%02x")
    {
        if (fall == 1)
        {
			format = "sonic_fall_%d"
			arg1 = (level.framecounter >> 3) % 3
        }         
        else
        {
            return base.stringformat(format, arg1)
        }
    }
    return base.stringformat(format, arg1)
}